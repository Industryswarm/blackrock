{

  "tags": {
    "allowUnknownTags": true
  },

  "source": {
    "include": [
      "./index.js",
      "./package.json",
      "./_definitions/jsdoc-namespaces",
      "./_definitions/jsdoc-tutorials",
      "./modules/app-engine",
      "./modules/cli",
      "./modules/configure",
      "./modules/core",
      "./modules/daemon",
      "./modules/data-access",
      "./modules/error-handler",
      "./modules/farm",
      "./modules/generator",
      "./modules/i18n",
      "./modules/installer",
      "./modules/jobs",
      "./modules/logger",
      "./modules/router",
      "./modules/sandbox",
      "./modules/utilities",
      "./interfaces/axon",
      "./interfaces/http",
      "./interfaces/nanomsg",
      "./interfaces/ssh",
      "./interfaces/websockets",
      "./interfaces/zeromq"
    ],
    "exclude": [
      "./modules/core/base/delete.js",
      "./interfaces/http/_support/delete.js"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\\\/|\\\\\\\\)_"
  },

  "plugins": [
    "plugins/markdown",
    "/usr/local/lib/node_modules/jsdoc-mermaid"
  ],

  "opts": {
    "template": "/usr/local/lib/node_modules/clean-jsdoc-theme",
    "readme": "./_definitions/jsdoc-namespaces/README.md",
    "tutorials": "./_definitions/jsdoc-tutorials",
    "encoding": "utf8",
    "destination": "_docs",
    "recurse": true,
    "verbose": true,
    "search": {},
    "theme_opts": {
      "theme": "light",
      "title": "Blackrock Application Server",
      "menu" : [
        {
          "title": " ",
          "link": "#"
        },
        {
          "title": " ",
          "link": "#"
        },
        {
          "title": " ",
          "link": "#"
        },
        {
          "title": " ",
          "link": "#"
        },
        {
          "title": " ",
          "link": "#"
        },
        {
          "title": "Blackrock Homepage",
          "link": "https://blackrock.industryswarm.com",
          "target": "_blank"
        },
        {
          "title": "Blackrock GitHub Repository",
          "link": "https://github.com/Industryswarm/isnode-blackrock",
          "target": "_blank"
        }
      ],
      "meta" : [
        {
          "name": "author",
          "content": "Darren Smith"
        },
        {
          "name": "description",
          "content": "The Blackrock Application Server"
        }
      ],
      "footer": "(c) Copyright, Darren Smith 2021 (<a href=\"https://www.darrensmith.com.au\">www.darrensmith.com.au</a>)"
    }
  }

}